{
  "name": "nickadams",
  "version": 2,
  "alias": "nickadams.now.sh",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "build": {
    "VUE_APP_SPACE_ID": "gwqnjet7ukqq",
    "VUE_APP_ACCESS_TOKEN": "b47a19fd395378fb5e76b2020fb91a0455d2b5ed7d4b9df29101e2e76fd00cc1"
  },
  "routes": [
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/fonts/(.*)",
      "dest": "/fonts/$1"
    },
    {
      "src": "^/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "^/favicon/.*",
      "dest": "/favicon/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VUE_APP_SPACE_ID": "gwqnjet7ukqq",
    "VUE_APP_ACCESS_TOKEN": "b47a19fd395378fb5e76b2020fb91a0455d2b5ed7d4b9df29101e2e76fd00cc1"
  }
}
